{
  "widgets": [
    {
      "type": "metric",
      "x": 6,
      "y": 5,
      "width": 6,
      "height": 6,
      "properties": {
        "metrics": [
          [ { "expression": "FILL(m2, 0)", "label": "Failed", "id": "e2", "region": "${region}", "color": "#d62728" } ],
          [ { "expression": "FILL(m1, 0)", "label": "Sucessful", "id": "e3", "region": "${region}", "color": "#2ca02c" } ],
          [ { "expression": "FILL(m1, 0)", "label": "Total", "id": "e1", "region": "${region}", "color": "#1f77b4" } ],
