          [ ".", ".", "environment", "${environment}", "service", "consumer", "OTelLib", "consumer.meter", "status", "failure", { "id": "m2", "label": "Failure", "visible": false, "region": "${region}" } ],
          [ "...", "success", { "id": "m3", "label": "Success", "visible": false, "region": "${region}" } ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "${region}",
        "title": "Records processed",
        "stat": "Sum",
        "period": 60,
        "yAxis": {
          "left": {
            "label": "Messages",
            "showUnits": false
          }
        },
        "legend": {
          "position": "bottom"
        }
      }
    },
